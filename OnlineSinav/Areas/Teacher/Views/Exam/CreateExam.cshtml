@model OnlineSinav.Areas.Teacher.ViewModels.Questions
@{
    ViewBag.Title = "CreateExam";
    var counter = 0;
}

<h2>CreateExam</h2>

<button type="button" class="btn btn-success" onclick="function addNewElement()"><i class="fa-plus">  Yeni Soru Ekle</i></button>


<div class="col-lg-12" id="questions">
    <div class="form-panel">
        @Html.Partial("_QuestionLayout", Model);
    </div>
</div>


    <script type="text/javascript">
        ($).ready(function addNewElement() {
            $('<div>').appendTo('#questions').load('Url.Action( "ReturnPartial", "Exam")');
        });

    </script>
